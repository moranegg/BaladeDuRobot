package fichiers;

public class Instance {
	private int[][] matrice;
	private int longueur;
	private int largeur;
	private Robot robot;

	public Instance(int lo, int la, int[][] m,Robot robot){
		this.largeur = la;
		this.longueur =lo;
		this.matrice = m;
		this.robot = robot;
		
	}
	/***Getters and Setter ***/
	public int[][] getMatrice(){
		return this.matrice;
	}

	public int getLongueur() {
		return longueur;
	}

	public int getLargeur() {
		return largeur;
	}

	/****************************/

}
